{
  "name": "@arction/lcjs-headless",
  "version": "1.0.0",
  "description": "Node JS support package for LightningChart JS.",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "dependencies": {
    "canvas": "^2.6.1",
    "gl": "^4.4.1",
    "jsdom": "^16.2.0",
    "@arction/lcjs": ">= 1.2.2"
  },
  "devDependencies": {
    "@types/gl": "^4.1.0",
    "@types/node": "^13.7.7",
    "@types/pngjs": "^3.4.1",
    "@types/sharp": "^0.24.0",
    "terser": "^4.6.6",
    "typedoc": "^0.16.11",
    "typescript": "^3.8.3"
  },
  "optionalDependencies": {
    "sharp": "^0.24.1",
    "pngjs": "^3.4.0"
  },
  "scripts": {
    "tsc": "tsc",
    "terser": "terser --output ./lib/index.js --config-file terser.json -- ./lib/index.js && terser --output ./lib/helpers.js --config-file terser.json -- ./lib/helpers.js",
    "build": "npm run tsc && npm run terser",
    "prepare": "npm run build",
    "docs": "typedoc --out docs --excludeNotExported"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:arction/lcjs/lcjs-headless.git"
  },
  "homepage": "https://www.arction.com/lightningchart-js/",
  "author": "Arction Ltd.",
  "license": "",
  "keywords": [
    "lightningchart",
    "lightning chart",
    "headless",
    "Arction",
    "typescript",
    "javascript",
    "GPU",
    "visualisation",
    "data visualisation",
    "data visualization",
    "node js"
  ],
  "files": [
    "lib/helpers.js",
    "lib/helpers.d.ts",
    "lib/index.js",
    "lib/index.d.ts"
  ]
}
